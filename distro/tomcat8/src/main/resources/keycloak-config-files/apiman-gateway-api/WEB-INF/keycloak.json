{
  "realm": "apiman",
  "resource": "apiman-gateway-api",
  "auth-server-url": "https://${keycloak.host}:${keycloak.port}/auth",
  "ssl-required": "external",
  "use-resource-role-mappings": false,
  "enable-cors": true,
  "cors-max-age": 1000,
  "cors-allowed-methods": "POST, PUT, DELETE, GET",
  "bearer-only": false,
  "enable-basic-auth": true,
  "expose-token": true,
  "credentials" : {
    "secret" : "${keycloak.apiman-gateway-api.secret}"
  },
  "connection-pool-size": 20,
  "principal-attribute": "preferred_username",
  "truststore": "/usr/local/tomcat/conf/apiman.jks",
  "truststore-password": "${keycloak.truststore-password}"
}